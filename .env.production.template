# Production Environment Template
# Copy this file to .env.production and replace placeholder values with actual credentials

# Production environment configuration
VITE_NODE_ENV=production

# Disable debug features in production
VITE_ENABLE_DEBUG=false
VITE_ENABLE_TEST_ROUTES=false
VITE_ENABLE_CONSOLE_LOGS=false

# Build optimization
VITE_BUILD_TIME=${BUILD_TIME}

# Production Security Settings
VITE_ENABLE_WEBHOOK_VALIDATION=true
VITE_REQUIRE_HTTPS=true
VITE_ENABLE_PAYMENT_LOGGING=true